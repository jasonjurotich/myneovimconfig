// https://github.com/microsoft/vscode/issues/42253
{
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.suggestSelection": "first",
  "editor.fontSize": 14,
  "editor.minimap.enabled": false,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.lineNumbers": "relative",
  "editor.renderLineHighlight": "line",
  "editor.folding": false,
  "editor.glyphMargin": true,
  "editor.wordWrap": "on",
  "editor.tabSize": 2,
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 2,
  "editor.detectIndentation": false,
  "editor.hideCursorInOverviewRuler": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "diffEditor.wordWrap": "on",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 100000,
  "notebook.output.textLineLimit": 50,
  "workbench.startupEditor": "none",
  "workbench.iconTheme": "vscode-icons",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "git.autofetch": true,
  "git.postCommitCommand": "push",
  "git.enableSmartCommit": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "push:command"
  ],
  "markdown.preview.fontSize": 15,
  "indentRainbow.updateDelay": 1000,
  "vsicons.dontShowNewVersionMessage": true,
  "backgroundCover.imagePath": "/Users/jj/Documents/RUSTDEV/rustglobal/GERMANCASTLE.jpg",
  "settingsSync.keybindingsPerPlatform": false,
  "remoteHub.commitDirectlyWarning": "off",
  "vim.vimrc.enable": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "t",
        "t"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "w",
        "w"
      ],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "println!(\"{:#?}\",$0);"
          }
        }
      ],
    },
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "<S-l>"
      ],
      "commands": [
        "workbench.action.navigateEditorGroups"
      ],
    },
    {
      "before": [
        "<S-k>",
      ],
      "commands": [
        "editor.action.showHover"
      ],
    },
    {
      "before": [
        "[",
        "d"
      ],
      "commands": [
        "editor.action.marker.next"
      ],
    },
    {
      "before": [
        "]",
        "d"
      ],
      "commands": [
        "editor.action.marker.prev"
      ],
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ],
    },
    {
      "before": [
        "v",
        "s"
      ],
      "commands": [
        "workbench.action.splitEditor"
      ],
    },
    {
      "before": [
        "g",
        "f"
      ],
      "commands": [
        "editor.action.goToReferences"
      ],
    },
    {
      "before": [
        "g",
        "b"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ],
    },
    {
      "before": [
        "g",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ],
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ],
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "commands": [
        "togs"
      ],
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "commands": [
        "workbench.action.focusSideBar"
      ],
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ],
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "spotify.playPause"
      ],
    },
    {
      "before": [
        "x"
      ],
      "after": [
        "C-b"
      ]
    },
    {
      "before": [
        "z"
      ],
      "after": [
        "C-f"
      ]
    }
  ],
  "vim.leader": "<space>",
  "explorer.confirmDelete": false,
  "files.exclude": {
    "": true,
    "**/.git": false
  },
  "commands.commands": {
    "togs": {
      "sequence": [
        {
          "command": "workbench.action.focusActiveEditorGroup"
        },
        {
          "command": "workbench.action.toggleSidebarVisibility"
        }
      ],
    },
  },
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "css",
    "git-commit",
    "graphql",
    "html",
    "jade",
    "json",
    "jsonc",
    "jupyter",
    "latex",
    "less",
    "markdown",
    "plaintext",
    "restructuredtext",
    "scss",
    "text",
    "yaml",
    "yml"
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "rust-analyzer.rustfmt.rangeFormatting.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "filePathBar.statusBarAlignment": "right",
  "spotify.showLyricsButton": false,
  "spotify.showSignInButton": false,
  "spotify.showSignOutButton": false,
  "tabnine.experimentalAutoImports": true,
  "clock.format": "ddd mmm dd yyyy HH:MM",
  "clock.alignment": "Right",
  "clock.priority": 1,
  "workbench.colorTheme": "Default Dark+",
  "markdown.experimental.editor.pasteLinks.enabled": true,
  "editor.experimental.pasteActions.enabled": true,
  "editor.linkedEditing": true,
  "rust-analyzer.cargo.unsetTest": [
    "tokio",
    "tokio-macros"
  ],
  "git.allowForcePush": true,
  "git.allowNoVerifyCommit": true,
  "git.branchProtectionPrompt": "alwaysCommit",
  "git.confirmEmptyCommits": false,
  "git.confirmForcePush": false,
  "git.confirmNoVerifyCommit": false,
  "resmon.show.cputemp": true,
  "resmon.disk.drives": false,
  "explorer.confirmDragAndDrop": false,
  "lldb.suppressUpdateNotifications": true,
  "markdown.validate.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "code-runner.executorMapByGlob": {
    "pom.xml": "cd $dir && mvn clean package"
  },
  "workbench.editor.enablePreview": false,
  "vscode-postgres.virtualFolders": [
    "functions"
  ],
  "workbench.editor.splitInGroupLayout": "vertical",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "indentRainbow.indicatorStyle": "light",
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.foreground": "#689ab3"
  },
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  "rust-analyzer.inlayHints.discriminantHints.enable": "always",
  "rust-analyzer.inlayHints.closingBraceHints.minLines": 1,
  "terminal.integrated.tabs.enabled": false,
  "workbench.editor.highlightModifiedTabs": true,
  "rust-analyzer.completion.privateEditable.enable": true,
  "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
  "workbench.activityBar.visible": false,
}
